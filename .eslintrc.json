{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "airbnb/hooks"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@emotion"],
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-indent": "warn",
    "quotes": ["warn", "double"],
    "semi": "warn",
    "arrow-body-style": "warn",
    "eol-last": "warn",
    "comma-dangle": "warn",
    "no-multiple-empty-lines": "warn",
    "react/prop-types": "off"
  }
}
